{
  "name": "@tevm/revm",
  "version": "1.0.0-next.142",
  "description": "REVM Rust-based EVM implementation for Tevm with WASM support",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "pkg",
    "!src/**/*.spec.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evmts/tevm-monorepo.git"
  },
  "license": "MIT",
  "scripts": {
    "build:dist": "tsup",
    "build:types": "tsc --project tsconfig.json --declaration --emitDeclarationOnly --outDir dist",
    "build:rust": "cargo build --target wasm32-unknown-unknown --release && wasm-bindgen --target web --out-dir pkg /Users/williamcory/tevm/main/dist/target/wasm32-unknown-unknown/release/tevm_revm.wasm",
    "build": "pnpm run build:rust && pnpm run build:dist && pnpm run build:types",
    "clean": "rimraf dist pkg",
    "generate:docs": "typedoc",
    "lint": "eslint src",
    "lint:deps": "depcheck",
    "lint:package": "publint",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:fib": "node test-fib.js",
    "bench": "node bench.js",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "esbuild": "^0.25.0",
    "rimraf": "^5.0.5",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
