{
  "name": "tevm_revm",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/revm/src",
  "tags": [],
  "targets": {
    "build:rust": {
      "cache": true,
      "outputs": ["{workspaceRoot}/packages/revm/pkg"],
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "cargo build --target wasm32-unknown-unknown --release",
          "~/.cargo/bin/wasm-bindgen --target web --out-dir pkg /Users/williamcory/tevm/main/dist/target/wasm32-unknown-unknown/release/tevm_revm.wasm"
        ],
        "cwd": "packages/revm"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "test": {
      "cache": true,
      "executor": "@monodon/rust:test",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/revm",
        "crate": "tevm_revm"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "cache": true,
      "executor": "@monodon/rust:lint",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/revm",
        "crate": "tevm_revm"
      }
    }
  }
}