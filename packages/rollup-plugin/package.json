{
  "name": "@evmts/rollup-plugin",
  "description": "A rollup plugin for evmts",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "src/index.ts",
  "type": "module",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/evmts/evmts-monorepo.git",
    "directory": "packages/plugin"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "typecheck": "tsc --noEmit",
    "envSchema": "ts-node src/scripts/envSchema.ts",
    "build": "tsup",
    "//test": "vitest",
    "//test:ui": "vitest --ui",
    "//test:run": "vitest run",
    "format": "rome format . --write",
    "format:check": "rome format .",
    "lint": "rome check . --apply-unsafe",
    "lint:check": "rome check . --verbose"
  },
  "peerDependencies": {
    "@babel/core": ">7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/traverse": "^7.21.4",
    "@types/babel__core": "^7.20.0",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@vitest/ui": "^0.30.1",
    "core": "link:@types/babel/core",
    "helper-plugin-utils": "link:@types/babel/helper-plugin-utils",
    "rome": "^12.0.0",
    "traverse": "link:@types/babel/traverse",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.0",
    "vitest": "^0.30.1",
    "zod": "^3.21.4"
  },
  "keywords": [
    "babel",
    "plugin",
    "solidity",
    "sol",
    "evmts"
  ],
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.20.2",
    "@babel/parser": "^7.21.4",
    "@evmts/plugin-internal": "workspace:^0.0.1",
    "@rollup/pluginutils": "^5.0.2",
    "@types/babel-core": "^6.25.7",
    "@types/fs-extra": "^11.0.1",
    "execa": "^7.1.1",
    "fs-extra": "^11.1.1",
    "globby": "^13.1.4",
    "pathe": "^1.1.0",
    "rollup": "^3.20.6"
  }
}
