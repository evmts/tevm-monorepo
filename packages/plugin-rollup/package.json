{
  "name": "@evmts/plugin-rollup",
  "description": "A rollup plugin for evmts",
  "version": "0.0.0",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "src/index.ts",
  "type": "module",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/evmts/evmts-monorepo.git",
    "directory": "packages/babel-plugin-sol"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "envSchema": "ts-node src/scripts/envSchema.ts",
    "build": "tsup",
    "//test": "vitest",
    "//test:ui": "vitest --ui",
    "//test:run": "vitest run",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix && prettier \"src/**/*.{ts,tsx}\" --write",
    "lint:check": "eslint \"**/*.{ts,tsx}\" && prettier --check \"src/**/*.{ts,tsx}\""
  },
  "peerDependencies": {
    "@babel/core": ">7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/traverse": "^7.20.13",
    "@types/babel__core": "^7.20.0",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@vitest/ui": "^0.28.4",
    "core": "link:@types/babel/core",
    "helper-plugin-utils": "link:@types/babel/helper-plugin-utils",
    "traverse": "link:@types/babel/traverse",
    "tsup": "^6.6.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.4",
    "zod": "^3.20.6"
  },
  "keywords": [
    "babel",
    "plugin",
    "solidity",
    "sol",
    "evmts"
  ],
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.20.2",
    "@babel/parser": "^7.20.15",
    "@types/babel-core": "^6.25.7",
    "ethers": "^6.0.2"
  }
}
