{
  "name": "@tevm/revm",
  "version": "1.0.0-next.28",
  "private": false,
  "description": "WASM JS bindings for REVM for use in Tevm",
  "keywords": [
    "solidity",
    "forge",
    "foundry",
    "sol",
    "typescript",
    "web3",
    "blockchain"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/evmts/tevm-monorepo.git",
    "directory": "packages/revm"
  },
  "license": "MIT",
  "contributors": [
    "Will Cory <willcory10@gmail.com>"
  ],
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/esm/revm.js",
      "require": "./dist/cjs/revm.js",
      "types": "./dist/esm/revm.d.ts",
      "default": "./dist/esm/revm.js"
    }
  },
  "main": "dist/cjs/revm.js",
  "module": "dist/esm/revm.js",
  "files": [
    "dist",
    "src",
    "cargo.toml",
    "Cargo.lock",
    "types"
  ],
  "scripts": {
    "//test": "vitest --coverage",
    "//test:coverage": "vitest run --coverage",
    "//test:run": "vitest run",
    "//test:ui": "vitest --ui",
    "build:wasm": "bun run build:esm && bun run build:cjs",
    "build:wasm:cjs": "wasm-pack build --target nodejs --out-dir ./dist/cjs --no-pack .",
    "build:wasm:dist": "echo 'building wasm with wasm-pack' && bun run build",
    "build:wasm:esm": "wasm-pack build --out-dir ./dist/esm --no-pack .",
    "clean": "rm -rf node_modules && rm -rf artifacts && rm -rf dist && rm -rf cache",
    "format": "rome format . --write",
    "format:check": "rome format .",
    "generate:wasm:docs": "bun nx build:esm && typedoc",
    "lint": "rome check . --apply-unsafe",
    "lint:check": "rome check . --verbose"
  },
  "devDependencies": {
    "@tevm/tsconfig": "workspace:^",
    "@tevm/tsupconfig": "workspace:^",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "abitype": "^0.10.2",
    "rome": "^12.1.3",
    "tsup": "^7.2.0",
    "typedoc": "^0.25.3",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.2.2",
    "viem": "^2.0.0",
    "vitest": "^0.34.6",
    "wagmi": "^1.4.5",
    "wasm-pack": "^0.12.1",
    "zod": "^3.22.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
