{
	"name": "@evmts/viem-effect",
	"version": "0.11.2",
	"private": false,
	"description": "A typesafe library for writing forge scripts in typescript",
	"keywords": [
		"solidity",
		"forge",
		"foundry",
		"sol",
		"typescript",
		"web3",
		"blockchain"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/evmts/evmts-monorepo.git",
		"directory": "viem-effect"
	},
	"license": "MIT",
	"contributors": [
		"Will Cory <willcory10@gmail.com>"
	],
	"type": "module",
	"exports": {
		".": {
			"default": "./dist/index.js",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./src/index.d.ts"
		}
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "types/index.d.ts",
	"files": [
		"dist",
		"src",
		"!src/**/*.spec.ts",
		"!src/**/*.spec.js"
	],
	"scripts": {
		"all": "bun run lint && bun run format && bun run build && bun run test",
		"build": "nx run-many --targets=build:dist --projects=@evmts/viem-effect ",
		"build:dist": "tsup",
		"clean": "rm -rf node_modules && rm -rf artifacts && rm -rf dist && rm -rf cache",
		"format": "rome format . --write",
		"format:check": "rome format .",
		"generate:docs": "typedoc",
		"generate:src": "bun run generateSrc.ts",
		"lint": "rome check . --apply-unsafe",
		"lint:check": "rome check . --verbose",
		"package:up": "pnpm up --latest",
		"//test": "vitest --coverage",
		"//test:coverage": "vitest run --coverage",
		"//test:run": "vitest run",
		"//test:ui": "vitest --ui",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@adraffy/ens-normalize": "1.9.4",
		"@effect/schema": "^0.42.0",
		"@noble/curves": "1.2.0",
		"@noble/hashes": "1.3.2",
		"@scure/bip32": "1.3.2",
		"@scure/bip39": "1.2.1",
		"@types/glob": "^8.1.0",
		"abitype": "0.9.8",
		"effect": "2.0.0-next.47",
		"isows": "1.0.3",
		"ws": "8.13.0"
	},
	"devDependencies": {
		"@babel/core": "^7.23.2",
		"@babel/preset-typescript": "^7.23.2",
		"@babel/template": "^7.22.15",
		"@babel/traverse": "^7.23.2",
		"@babel/types": "^7.23.0",
		"@evmts/tsconfig": "workspace:^",
		"@vitest/coverage-v8": "^0.34.6",
		"@vitest/ui": "^0.34.6",
		"abitype": "^0.9.8",
		"babel-cli": "^6.26.0",
		"glob": "^10.3.10",
		"rome": "^12.1.3",
		"tsup": "^7.2.0",
		"typedoc": "^0.25.2",
		"typedoc-plugin-markdown": "^3.16.0",
		"typescript": "^5.2.2",
		"viem": "^1.14.0",
		"vitest": "^0.34.6",
		"wagmi": "^1.4.2",
		"zod": "^3.22.2"
	},
	"peerDependencies": {
		"viem": ">1.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
