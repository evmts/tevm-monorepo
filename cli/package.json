{
	"name": "@tevm/cli",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"main": "dist/cli.js",
	"module": "dist/cli.js",
	"sideEffects": true,
	"types": "dist/cli.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/cli.d.ts",
				"default": "./dist/cli.js"
			}
		},
		"./package.json": "./package.json"
	},
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsx src/cli.tsx",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@tanstack/react-query": "^5.67.3",
		"@tevm/actions": "workspace:^",
		"@tevm/base-bundler": "workspace:^",
		"@tevm/bundler-cache": "workspace:^",
		"@tevm/common": "workspace:^",
		"@tevm/config": "workspace:^",
		"@tevm/jsonrpc": "workspace:^",
		"@tevm/memory-client": "workspace:^",
		"@tevm/server": "workspace:^",
		"@tevm/utils": "workspace:^",
		"@types/glob": "^8.1.0",
		"@types/json-bigint": "^1.0.4",
		"effect": "3.13.2",
		"figures": "^6.1.0",
		"glob": "^11.0.1",
		"ink": "^4.1.0",
		"ink-big-text": "^2.0.0",
		"ink-gradient": "^3.0.0",
		"ink-select-input": "^5.0.0",
		"ink-spinner": "^5.0.0",
		"ink-text-input": "^5.0.1",
		"json-bigint": "^1.0.0",
		"pastel": "^2.0.0",
		"react": "^18.2.0",
		"solc": "^0.8.23",
		"tiny-jsonc": "^1.0.2",
		"viem": "^2.23.11",
		"zod": "^3.21.4",
		"zustand": "^4.5.6"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/ink-big-text": "^1.2.4",
		"@types/ink-gradient": "^2.0.4",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.54.2"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
