{
	"name": "@tevm/cli",
	"version": "1.0.0-next.148",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"main": "dist/cli.js",
	"module": "dist/cli.js",
	"sideEffects": true,
	"types": "dist/cli.d.ts",
	"exports": {
		".": {
			"types": "./dist/cli.d.ts",
			"default": "./dist/cli.js"
		},
		"./package.json": "./package.json"
	},
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "pnpm run build:app",
		"build:app": "tsc --skipLibCheck && mkdir -p dist/utils && cp src/utils/bun.lockb dist/utils/",
		"dev": "tsx src/cli.tsx",
		"format": "biome format . --write",
		"lint": "biome check . --write --unsafe",
		"lint:check": "biome check . --verbose",
		"lint:deps": "bunx depcheck",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@tanstack/react-query": "^5.90.20",
		"@tevm/actions": "workspace:^",
		"@tevm/base-bundler": "workspace:^",
		"@tevm/bundler-cache": "workspace:^",
		"@tevm/common": "workspace:^",
		"@tevm/config": "workspace:^",
		"@tevm/jsonrpc": "workspace:^",
		"@tevm/memory-client": "workspace:^",
		"@tevm/server": "workspace:^",
		"@tevm/utils": "workspace:^",
		"@types/json-bigint": "^1.0.4",
		"effect": "3.19.16",
		"figures": "^6.1.0",
		"glob": "^13.0.1",
		"ink": "^6.6.0",
		"ink-big-text": "^2.0.0",
		"ink-gradient": "^4.0.0",
		"ink-select-input": "^6.2.0",
		"ink-spinner": "^5.0.0",
		"ink-text-input": "^6.0.0",
		"json-bigint": "^1.0.0",
		"pastel": "^4.0.0",
		"react": "^19.2.4",
		"solc": "^0.8.33",
		"viem": "^2.45.2",
		"zod": "^4.3.6",
		"zustand": "^5.0.11"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^8.1.0",
		"@types/ink-big-text": "^1.2.4",
		"@types/ink-gradient": "^2.0.4",
		"@types/react": "^19.2.13",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^6.4.1",
		"chalk": "^5.6.2",
		"ink-testing-library": "^4.0.0",
		"prettier": "^3.8.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"xo": "^1.2.3"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
