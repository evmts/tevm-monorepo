		"test:run": "vitest run",
		"test:ui": "vitest --ui"
	},
	"dependencies": {
		"ink": "^4.4.1",
		"pastel": "^2.0.0",
		"react": "^18.2.0",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"@vitest/coverage-v8": "^0.34.6",
		"@vitest/ui": "^0.34.6",
		"vitest": "^0.34.6"
	},
	"engines": {
		"node": ">=16"
	}
