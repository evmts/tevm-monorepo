{
	"name": "@evmts/cli",
	"version": "0.0.0",
	"license": "MIT",
	"type": "module",
	"bin": {
		"create-evmts-app": "dist/create-evmts-app.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc && chmod +x dist/create-evmts-app.js",
		"dev": "tsx src/create-evmts-app.tsx example && rm -rf example || rm -rf example",
		"test": "vitest --coverage",
