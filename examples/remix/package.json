    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "isbot": "^4.1.0",
    "lucide-react": "^0.482.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.6.0",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
