{
  "name": "tevm_bundler_rs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "bundler-packages/bundler-rs/src",
  "tags": [],
  "targets": {
    "build:rust": {
      "cache": false,
      "outputs": ["{workspaceRoot}/bundler-packages/bundler-rs"],
      "executor": "@monodon/rust:napi",
      "options": {
        "dist": "dist/bundler-rs",
        "jsFile": "index.js"
      },
      "configurations": {
        "production": {
          "dist": "dist/bundler-rs",
          "release": true
        }
      }
    },
    "test:coverage": {
      "cache": false,
      "executor": "@monodon/rust:test",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/bundler-rs"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    }
  }
}
