{
  "name": "@tevm/resolutions-rs",
  "version": "0.1.0",
  "private": true,
  "description": "Rust implementation of solidity resolution",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/evmts/tevm-monorepo.git",
    "directory": "bundler-packages/resolutions-rs"
  },
  "license": "MIT",
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node test/index.js",
    "example": "node examples/usage.js"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.15.2"
  },
  "engines": {
    "node": ">= 14"
  },
  "napi": {
    "binaryName": "tevm_resolutions_rs",
    "packageName": "@tevm/resolutions-rs",
    "targets": [
      "aarch64-apple-darwin",
      "aarch64-unknown-linux-gnu",
      "aarch64-unknown-linux-musl",
      "aarch64-pc-windows-msvc",
      "x86_64-unknown-linux-musl",
      "x86_64-apple-darwin",
      "i686-pc-windows-msvc",
      "wasm32-wasip1-threads"
    ],
    "features": ["napi"]
  },
  "files": [
    "index.js",
    "index.d.ts",
    "README.md",
    "bindings"
  ]
}
