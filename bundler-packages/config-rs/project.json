{
  "name": "tevm_config_rs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "bundler-packages/config-rs/src",
  "tags": [],
  "targets": {
    "build:rust": {
      "cache": true,
      "outputs": ["{workspaceRoot}/bundler-packages/config-rs"],
      "executor": "@monodon/rust:build",
      "configurations": {
        "production": {
          "dist": "dist/config-rs",
          "release": true
        }
      }
    },
    "test:coverage": {
      "cache": true,
      "executor": "@monodon/rust:test",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/config-rs"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "cache": true,
      "executor": "@monodon/rust:lint",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/config-rs"
      }
    }
  }
}
