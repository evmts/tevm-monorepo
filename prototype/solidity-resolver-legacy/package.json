{
  "name": "@evmts/bundler-legacy",
  "version": "0.0.1",
  "private": false,
  "description": "The internal code EVMts plugins use to resolve .sol imports",
  "keywords": [
    "plugin",
    "solidity",
    "sol",
    "evmts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/evmts/evmts-monorepo.git",
    "directory": "plugins/modules-legacy"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "format": "rome format . --write",
    "format:check": "rome format .",
    "lint": "rome check . --apply-unsafe",
    "//lint:check": "rome check . --verbose",
    "package:up": "pnpm up --latest",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "execa": "^7.1.1",
    "fs-extra": "^11.1.1",
    "globby": "^13.1.4",
    "pathe": "^1.1.0"
  },
  "devDependencies": {
    "@vitest/ui": "^0.31.0",
    "rome": "^12.1.3",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.31.0",
    "zod": "^3.21.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
