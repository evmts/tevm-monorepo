{
  "name": "@evmts/unplugin",
  "version": "0.0.1",
  "private": false,
  "description": "A generic plugin that can be used in esbuild, webpack, vite, and rollup",
  "keywords": [
    "vite",
    "rollup",
    "esbuild",
    "webpack",
    "rspack",
    "plugin",
    "solidity",
    "sol",
    "evmts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/evmts/evmts-monorepo.git",
    "directory": "plugins/unplugin"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "//test": "vitest",
    "//test:run": "vitest run",
    "//test:ui": "vitest --ui",
    "build": "tsup",
    "clean": "rm -rf node_modules && rm -rf artifacts && rm -rf dist && rm -rf cache",
    "envSchema": "ts-node src/scripts/envSchema.ts",
    "format": "rome format . --write",
    "format:check": "rome format .",
    "lint": "rome check . --apply-unsafe",
    "lint:check": "rome check . --verbose",
    "package:up": "pnpm up --latest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@evmts/solidity-resolver": "workspace:^",
    "@rollup/pluginutils": "^5.0.2",
    "execa": "^7.1.1",
    "fs-extra": "^11.1.1",
    "globby": "^13.1.4",
    "pathe": "^1.1.0",
    "rollup": "^3.22.0",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@vitest/ui": "^0.31.1",
    "rome": "^12.1.2",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vitest": "^0.31.1",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "@babel/core": ">7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
