		"release": "npm run build && pnpm dlx @vscode/vsce publish --pre-release"
	},
	"devDependencies": {
		"@tevm/lsp": "workspace:^",
		"@tevm/tsconfig": "workspace:^",
		"@tevm/tsupconfig": "workspace:^",
		"@types/vscode": "^1.109.0",
		"@volar/language-server": "~2.4.28",
		"@volar/vscode": "~2.4.28",
		"esbuild": "^0.27.3",
		"vscode-languageclient": "^9.0.1"
	}
}
