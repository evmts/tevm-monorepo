{
  "private": true,
  "name": "@evmts/vscode",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/volarjs/starter.git",
    "directory": "packages/vscode"
  },
  "categories": [
    "Programming Languages"
  ],
  "displayName": "EVMts Language Features",
  "description": "EVMts Language Features",
  "author": "volarjs",
  "publisher": "volarjs",
  "engines": {
    "vscode": "^1.67.0"
  },
  "activationEvents": [
    "onLanguage:sol"
  ],
  "main": "./dist/client.js",
  "contributes": {
    "languages": [
      {
        "id": "sol",
        "extensions": [
          ".sol"
        ]
      }
    ]
  },
  "scripts": {
    "prebuild": "cd ../.. && npm run build",
    "build": "node scripts/build -- --minify",
    "watch": "npm run build -- --watch",
    "pack": "npm run build && vsce package --pre-release",
    "release": "npm run build && vsce publish --pre-release",
    "package:up": "pnpm up --latest"
  },
  "devDependencies": {
    "@types/vscode": "1.79.1",
    "@volar/language-server": "1.7.10",
    "@volar/vscode": "1.7.10",
    "esbuild": "0.18.11",
    "vsce": "^2.15.0",
    "vscode-languageclient": "^8.1.0"
  },
  "dependencies": {
    "@evmts/language-server": "workspace:^"
  }
}
