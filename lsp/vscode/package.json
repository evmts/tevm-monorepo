{
	"private": true,
	"name": "@tevm/vscode",
	"version": "1.0.0-next.148",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/evmts/tevm-monorepo.git",
		"directory": "lsp/vscode"
	},
	"categories": [
		"Programming Languages"
	],
	"displayName": "Tevm Language Features",
	"description": "Tevm Language Features",
	"author": "tevm",
	"publisher": "tevm",
	"engines": {
		"vscode": "^1.82.0"
	},
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:typescript"
	],
	"main": "./dist/client.js",
	"scripts": {
		"build": "node scripts/build -- --minify",
		"watch": "npm run build -- --watch",
