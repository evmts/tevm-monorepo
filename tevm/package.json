{
	"name": "tevm",
	"version": "1.0.0-next.12",
	"private": false,
	"description": "A typesafe Ethereum Virtual Machine (EVM) toolkit written in TypeScript",
	"type": "module",
	"keywords": [
		"solidity",
		"tevm",
		"evm",
		"sol",
		"typescript",
		"web3",
		"blockchain"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/evmts/tevm-monorepo.git",
		"directory": "tevm"
	},
	"license": "MIT",
	"contributors": [
		"Will Cory <willcory10@gmail.com>"
	],
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./index.d.ts",
				"default": "./index.js"
			},
			"require": {
				"types": "./index.d.cts",
				"default": "./index.cjs"
			}
		},
		"./api": {
			"import": {
				"types": "./api/index.d.ts",
				"default": "./api/index.js"
			},
			"require": {
				"types": "./api/index.d.cts",
				"default": "./api/index.cjs"
			}
		},
		"./bundler": {
